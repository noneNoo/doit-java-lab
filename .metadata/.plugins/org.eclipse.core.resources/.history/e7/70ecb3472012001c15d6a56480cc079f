package chapter5;

import java.text.SimpleDateFormat;
import java.util.Date;

public class Alone3OrderInfo {
	public static void main(String[] args) {
		
		OrderInfo order1 = new OrderInfo();
		
		
		Date today = new Date();
		SimpleDateFormat dayFormat = new SimpleDateFormat("2018-03-12");
		
		order1.orderDate = dayFormat.format(today);
		
		System.out.println(order1.orderDate);
	}

}
