package chapter5;

public class Alone2 {
	
	public static void main(String[] args) {

		public class Person {
			int age;
			String name;
			boolean isMarried;
			int children;
		}

		
		Person person1 = new Person();
		
		person1.age = 40;
		person1.name = "James";
		person1.isMarried = true;
		person1.children = 3;
				

	}

}
