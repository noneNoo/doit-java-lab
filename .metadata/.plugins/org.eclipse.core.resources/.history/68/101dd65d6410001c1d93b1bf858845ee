package chapter4;

public class Alone2 {

	public static void main(String[] args) {
		int floor = 5;
		String place;
		
		switch(floor) {
		case 1:
			place = "약국";
			System.out.println(place + "입니다");
			break;
		case 2:
			place = "정형외과";
			System.out.println(place + "입니다");
			break;
		case 3:
			place = "피부과";
			System.out.println(place + "입니다");
			break;
		case 4:
			place = "치과";
			System.out.println(place + "입니다");
			break;
		case 5:
			place = "헬스 클럽";
			System.out.println(place + "입니다");
			break;
		default:
			System.out.println("잘못된 층입니다.");
			break;
		}
	}

}
