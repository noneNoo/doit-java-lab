package chapter5;

public class Alone2 {
	
	public class Person {
		int age;
		String name;
		boolean isMarried;
		int children;
	}

	public static void main(String[] args) {
		Person Person1 = new Person();
		
		Person1.age = 30;
		Person1.name = "James";
		Person1.isMarried = true;
		Person1.children = 2;
				

	}

}
