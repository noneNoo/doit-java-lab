package chapter4;

public class Quiz4 {
	public static void main(String[] args) {

		final int HEIGHT = 5;
		int spaceCount = HEIGHT / 2 + 1;
		int starCount = 1;
		
		// 높이만큼 반복된다
		for(int i = 0; i < HEIGHT; i++) {
			for(int j = 0; j < spaceCount; j++) {
				System.out.println('-');
			}
		}
	}
}
