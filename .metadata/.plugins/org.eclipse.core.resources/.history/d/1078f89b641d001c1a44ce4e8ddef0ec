package chapter9.quiz;

public abstract class IdolType {
	public void rest() {
		System.out.println("아이돌이 휴식합니다.");
	}

	public boolean getPassed() {
		return isPassed;
	}

	public void setPassed(boolean isPassed) {
		this.isPassed = isPassed;
	}
	
	public abstract void training();
	public abstract void audition();
	public void go() {
		training();
		rest();
		audition();
	}

}
