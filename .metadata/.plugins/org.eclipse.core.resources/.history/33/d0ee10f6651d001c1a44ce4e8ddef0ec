package chapter9.quiz;
import java.util.ArrayList;

public class WingTest {

	public static void main(String[] args) {
		ArrayList<Idol> idolList = new ArrayList<Idol>();
		idolList.add(new Idol("치요코", VocalIdol));
		idolList.add(new DanceIdol("나츠하"));
		idolList.add(new VisualIDol("린제"));
		for(Idol idol : idolList) {
			idol.produce();
			System.out.println("==========");
		}
	}

}
