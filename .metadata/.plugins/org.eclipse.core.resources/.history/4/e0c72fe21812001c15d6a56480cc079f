package chapter5;

public class Alone2Person {

		int age;
		String name;
		boolean isMarried;
		int children;
	
	public static void main(String[] args) {

		Alone2Person personJ = new Alone2Person();
		
		personJ.age = 40;
		personJ.name = "James";
		personJ.isMarried = true;
		personJ.children = 3;
				

	}

}
