package chapter9.quiz;

public class Idol {
	public String name;
	private IdolType type;
	private boolean isPass;
	
	Idol(String name, IdolType type) {
		this.name = name;
		this.type = type;
	}
	
	public void produce(int count) {
		type.go(count);
	}

	public boolean isPass() {
		return isPass;
	}

	public void setPass(boolean isPass) {
		this.isPass = isPass;
	}

}
