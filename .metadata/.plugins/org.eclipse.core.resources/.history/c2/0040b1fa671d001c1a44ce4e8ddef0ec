package chapter9.quiz;

public abstract class IdolType {
	public void rest() {
		System.out.println("아이돌이 휴식합니다.");
	}
	
	public abstract void training();
	public abstract boolean audition();
	public void go(int count) {
		for(int i = 0; i < count; i++) {
			training();
		}
		rest();
		audition();
	}

	
	public static void main(String[] args) {
		VocalIdol vo = new VocalIdol();
		vo.training();
		vo.audition();
	}
}
