package chapter9.quiz;

import java.util.Random;

public class VocalIdol extends IdolType {
	Random rand = new Random();
	private boolean isPassed = false;
	private int vocalPoint = 0;
	
	@Override
	public void training() {
		System.out.println("보컬 트레이닝을 실시합니다.");
		// 10 이상 20 이하의 값이 랜덤으로 올라간다
		int upPoint = rand.nextInt(10) + 10;
		System.out.println("RAND 결과값 : " + upPoint + " 가 올랐습니다.");
	}

	@Override
	public boolean audition() {
		System.out.println("보컬 오디션을 실시합니다.");
		if() {
			
		}
	}
	
	public static void main(String[] args) {
		VocalIdol vo = new VocalIdol();
		vo.training();
	}

	public boolean isPassed() {
		return isPassed;
	}

	public void setPassed(boolean isPassed) {
		this.isPassed = isPassed;
	}
}
