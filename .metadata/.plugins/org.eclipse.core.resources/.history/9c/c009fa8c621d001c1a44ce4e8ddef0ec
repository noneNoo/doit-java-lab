package chapter9.quiz;

import java.util.Random;

public class VocalIdol extends IdolType {

	@Override
	public void training() {
		System.out.println("보컬 트레이닝을 실시합니다.");
	}

	@Override
	public void audition() {
		System.out.println("보컬 오디션을 실시합니다.");
		Random rand = new Random();
		System.out.println("RAND 결과값 : " + rand.nextInt(30));
	}
}
