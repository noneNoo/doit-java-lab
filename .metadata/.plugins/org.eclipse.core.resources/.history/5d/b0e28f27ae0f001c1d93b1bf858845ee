package chapter3;

public class OperationEx5 {

	public static void main(String[] args) {
		int num = 0B00000101;
		System.out.println(num);
		
		System.out.println(num << 2);
		System.out.println(num >> 2);
		System.out.println(num >>> 2);

		System.out.println(num);
		
		num = num << 2;

		System.out.println(num);
		
		int num1 = 2;
		int num2 = 10;
		
		System.out.println(num1&num2);
	}

}
