package chapter9.quiz;

public class Idol {
	public String name;
	private IdolType type;
	private boolean isPassed;
	
	Idol(String name, IdolType type) {
		this.name = name;
		this.type = type;
	}
	
	public void produce(int count) {
		type.go(count);
	}

	public void setIsPassed(boolean isPassed) {
		this.isPassed = isPassed;
	}

}
