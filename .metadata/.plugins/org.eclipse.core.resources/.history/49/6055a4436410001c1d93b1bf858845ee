package chapter4;

public class Alone2 {

	public static void main(String[] args) {
		int floor = 5;
		
		switch(floor) {
		case 1:
			System.out.println("약국입니다");
			break;
		case 2:
			System.out.println("약국입니다");
			break;
		case 3:
			System.out.println("약국입니다");
			break;
		case 4:
			System.out.println("약국입니다");
			break;
		case 5:
			System.out.println("약국입니다");
			break;
		default:
			System.out.println("잘못된 층입니다.");
			break;
		}
	}

}
