package chapter9.quiz;
import java.util.ArrayList;

public class WingTest {

	public static void main(String[] args) {
		ArrayList<Idol> idolList = new ArrayList<Idol>();
		idolList.add(new VocalIdol("치요코", "SSR"));
		idolList.add(new DanceIdol("나츠하", "SR"));
		idolList.add(new VisualIDol("린제", "R"));
		for(Idol idol : idolList) {
			idol.produce();
			System.out.println("==========");
		}
	}

}
